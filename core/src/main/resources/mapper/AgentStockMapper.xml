<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zhancheng.core.dao.AgentStockMapper">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.zhancheng.core.entity.AgentStock" id="agentStockMap">
        <result property="id" column="id"/>
        <result property="agentId" column="agent_id"/>
        <result property="type" column="type"/>
        <result property="ptNumber" column="pt_number"/>
        <result property="stock" column="stock"/>
        <result property="totalPrice" column="total_price"/>
        <result property="gmtCreate" column="gmt_create"/>
        <result property="gmtModified" column="gmt_modified"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
    id,agent_id,type,pt_number,stock,total_price,gmt_create,gmt_modified,    </sql>


</mapper>