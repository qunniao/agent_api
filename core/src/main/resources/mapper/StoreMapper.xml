<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zhancheng.core.dao.StoreMapper">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.zhancheng.core.entity.Store" id="storeMap">
        <result property="sid" column="sid"/>
        <result property="agentId" column="agent_id"/>
        <result property="storeName" column="store_name"/>
        <result property="storeCover" column="store_cover"/>
        <result property="adCover" column="ad_cover"/>
        <result property="storeIntro" column="store_intro"/>
        <result property="status" column="status"/>
        <result property="isDeleted" column="is_deleted"/>
        <result property="gmtCreate" column="gmt_create"/>
        <result property="gmtModified" column="gmt_modified"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
    sid,agent_id, store_name,store_cover,ad_cover,store_intro,status,is_deleted,gmt_create,gmt_modified  </sql>


</mapper>